{
  "files.exclude": {
    "amplify/.config": true,
    "amplify/**/*-parameters.json": true,
    "amplify/**/amplify.state": true,
    "amplify/**/transform.conf.json": true,
    "amplify/#current-cloud-backend": true,
    "amplify/backend/amplify-meta.json": true,
    "amplify/backend/awscloudformation": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "cSpell.enabled": true,
  "cSpell.words": [
    "CMAKE",
    "Dodd's",
    "Elgato",
    "React's",
    "apos",
    "boldsymbol",
    "brkt",
    "buttontext",
    "expec",
    "frontmatter",
    "gapi",
    "helvetica",
    "ints",
    "libevent",
    "lmpi",
    "lopencv",
    "mathbb",
    "mathcal",
    "mathclose",
    "mathopen",
    "mathrm",
    "mpirun",
    "neue",
    "ompi",
    "operatorname",
    "pylintrc",
    "textfilter"
  ],
  "eslint.validate": [
    "javascript"
  ],
  "cSpell.ignoreWords": [
    "openmpi"
  ]
}